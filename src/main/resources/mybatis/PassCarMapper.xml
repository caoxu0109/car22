<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cao.car.dao.PassCarMapper">

    <select id="Findall" resultType="PassCar">
        select * from passCar
    </select>
    <update id="changetitle" parameterType="PassCar">
        update passcar set title = #{title} where time=#{time}
    </update>
</mapper>